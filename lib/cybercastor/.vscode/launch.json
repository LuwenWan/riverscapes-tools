{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "inputs": [
    {
      "type": "promptString",
      "id": "JOB_NAME",
      "description": "Job Name?",
      "default": "Riverscapes" // Really small HUC
    },
    {
      "type": "promptString",
      "id": "JOB_TYPE",
      "description": "Type of Job?",
      "default": "vbet"
    },
    {
      "type": "promptString",
      "id": "TAGS",
      "description": "What Tags?",
      "default": "CyberCastor,NRCS,CEAP"
    },
    {
      "type": "promptString",
      "id": "ORGANIZATION",
      "description": "What Organization?",
      "default": "a52b8094-7a1d-4171-955c-ad30ae935296"
    },
    {
      "type": "promptString",
      "id": "HUCs",
      "description": "What HUCs?",
      "default": "17060304"
    },
  ],
  "configurations": [
    {
      "name": "ðŸ¦« Monitor",
      "type": "python",
      "request": "launch",
      "module": "cybercastor.monitor",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
      "args": [
        "{env:CC_API_URL}",
        "{env:USERNAME}",
        "{env:PASSWORD}",
        // "--download_running",
        "--verbose"
      ]
    },
    { 
      "name": " CreateJob",
      "type": "python",
      "request": "launch",
      "module": "cybercastor.create_job",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
      "args": [
        "${input:JOB_NAME}",
        "${input:JOB_TYPE}",
        "${input:HUCs}",
        "${input:TAGS}",
        "${input:ORGANIZATION}"
      ]
    },
    {
      "name": "ðŸ¦« AddJob",
      "type": "python",
      "request": "launch",
      "module": "cybercastor.add_job",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
      "args": [
        "${workspaceFolder}/jobs/",
        "{env:CC_API_URL}",
        "{env:USERNAME}",
        "{env:PASSWORD}",
        "--verbose"
      ]
    },
    {
      "name": "ðŸ¦« dump_sqlite",
      "type": "python",
      "request": "launch",
      "module": "cybercastor.dump_sqlite",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
      // Add NATIONAL_PROJECT, INPUTS_FOLDER, DOWNLOAD_FOLDER to your .env file
      "args": [
        "{env:DATA_ROOT}/warehouse_report",
        "{env:CC_API_URL}",
        "{env:USERNAME}",
        "{env:PASSWORD}",
        "--verbose"
      ]
    },
